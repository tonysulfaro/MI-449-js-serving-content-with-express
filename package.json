{
  "name": "serving-content-with-express",
  "version": "1.0.0",
  "description": "Something neat for Erik to look at :)",
  "engines": {
    "node": "12.x"
  },
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "node test.js",
    "dev:server": "live-server",
    "dev:sass": "node-sass source/scss/style.scss --watch --output=compiled/css --output-style=compressed --source-map=true",
    "dev": "run-p dev:*",
    "build:htmlhint": "htmlhint *.html",
    "build:stylelint": "stylelint \"source/scss/*.scss\"",
    "build:standard": "standard \"*.js\"",
    "build:sass": "node-sass source/scss/main.scss --output=public/stylesheets/ --output-style=compressed --source-map=true",
    "build": "run-s build:*"
  },
  "dependencies": {
    "ejs": "^2.5.6",
    "express": "^4.15.2"
  },
  "devDependencies": {
    "htmlhint": "^0.11.0",
    "live-server": "^1.2.1",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "request": "^2.81.0",
    "standard": "^14.3.1",
    "stylelint": "^13.1.0",
    "stylelint-config-standard": "^20.0.0",
    "tape": "^4.7.0"
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tonysulfaro/MI-449-js-serving-content-with-express"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "license": "MIT"
}
